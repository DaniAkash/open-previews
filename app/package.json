{
  "name": "open-previews",
  "version": "0.0.1",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:vite\" \"npm run wundergraph\" \"npm run browser\"",
    "check": "tsc --noEmit",
    "clean": "rimraf ./.next",
    "wundergraph": "wunderctl up",
    "browser": "wait-on \"http-get://localhost:5173\" && wait-on \"http-get://localhost:9991\" && open-cli http://localhost:5173",
    "dev:vite": "onchange 'src/**/*' -- pnpm run build:vite",
    "build:wundergraph": "wunderctl generate",
    "build:vite": "vite build",
    "build": "npm run build:wundergraph && npm run build:vite",
    "preview": "vite preview"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.0.8",
    "@macaron-css/core": "^1.2.0",
    "@macaron-css/react": "^1.4.0",
    "@macaron-css/vite": "^1.4.2",
    "@nanostores/react": "^0.7.1",
    "@radix-ui/colors": "^0.1.8",
    "@radix-ui/react-popover": "^1.0.6",
    "@radix-ui/react-toolbar": "^1.0.4",
    "@stitches/react": "^1.2.8",
    "@vanilla-extract/css": "^1.7.1",
    "@vanilla-extract/recipes": "0.2.5",
    "@wundergraph/sdk": "^0.164.1",
    "@wundergraph/swr": "^0.15.7",
    "framer-motion": "^10.12.16",
    "graphql": "^16.6.0",
    "jose": "^4.14.4",
    "jsonwebtoken": "^9.0.0",
    "localforage": "^1.10.0",
    "nanostores": "^0.9.1",
    "nodemon": "^2.0.22",
    "rangy": "^1.3.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "swr": "^2.1.5"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node-fetch": "^2.6.2",
    "@types/rangy": "^0.0.35",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@vanilla-extract/vite-plugin": "^3.8.2",
    "@vitejs/plugin-react": "^1.0.7",
    "concurrently": "^6.0.0",
    "node-fetch": "^2.6.7",
    "onchange": "^7.1.0",
    "open-cli": "^7.0.1",
    "typescript": "^4.6.4",
    "vite": "^2.7.0",
    "vite-plugin-dts": "^2.3.0",
    "wait-on": "^6.0.0",
    "zod": "^3.21.4"
  }
}
