{
  "name": "open-previews",
  "version": "0.0.1",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:vite\" \"npm run wundergraph\" \"npm run browser\"",
    "check": "tsc --noEmit",
    "clean": "rimraf ./.next",
    "wundergraph": "wunderctl up",
    "browser": "wait-on \"http-get://localhost:5173\" && wait-on \"http-get://localhost:9991\" && open-cli http://localhost:5173",
    "dev:vite": "vite build --watch",
    "build:wundergraph": "wunderctl generate",
    "build:vite": "vite build",
    "build": "npm run build:wundergraph && npm run build:vite",
    "preview": "vite preview"
  },
  "dependencies": {
    "@wundergraph/sdk": "^0.160.0",
    "@wundergraph/swr": "^0.15.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "swr": "^2.1.5"
  },
  "devDependencies": {
    "@types/node-fetch": "^2.6.2",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.0.0",
    "autoprefixer": "^10.4.14",
    "concurrently": "^6.0.0",
    "node-fetch": "^2.6.7",
    "open-cli": "^7.0.1",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.2",
    "typescript": "^4.6.4",
    "vite": "^3.0.0",
    "vite-plugin-dts": "^2.3.0",
    "wait-on": "^6.0.0",
    "zod": "^3.21.4"
  }
}
